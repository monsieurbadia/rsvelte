{
  "name": "front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "fmt": "prettier --write --plugin-search-dir=. 'src/**/*.{js,svelte,scss,css,json}'",
    "lint:code": "eslint 'src/**/*.{js,svelte}'",
    "lint:style": "stylelint 'src/**/*.{css,scss,svelte}'",
    "start": "sirv public --no-clear",
    "storybook": "start-storybook --no-manager-cache -p 6006 -s public",
    "storybook:build": "build-storybook -s public",
    "test:unit": "jest --watchAll",
    "test:clear": "jest --clearCache"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/svelte": "^6.3.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@wasm-tool/rollup-plugin-rust": "^1.0.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.2.4",
    "jest-transform-svelte": "^2.1.1",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass-loader": "^12.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "svelte": "^3.0.0",
    "svelte-eslint-parser": "^0.4.6",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.0.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
